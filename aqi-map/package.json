{
    "project": {
        "authors": [
            "rsbaumann"
        ],
        "title": "aqi-map-hyperlocal",
        "description": "Hyperlocal, realtime, global air quality map from various AQI sensor data sources",
        "tags": [
            "aqi",
            "map",
            "wildfire",
            "smoke"
        ],
        "created": "08-14-2021",
        "published": "08-14-2021",
        "thumbnail": "tbd",
        "version": "0.1.0"
    },
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "mkdir -p build && cp index.html build/ && browserify -p esmify index.js > build/bundle.js",
        "start": "budo index.js:bundle.js --live --open "
    },
    "browserify": {
        "transform": [
            [
                "babelify",
                {
                    "presets": [
                        "@babel/preset-env"
                    ]
                }
            ]
        ]
    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.0",
        "babel-eslint": "^10.1.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babelify": "^10.0.0",
        "esmify": "^2.1.1",
        "esminify": "^4.0.1"
    },
    "dependencies": {
        "@babel/core": "^7.10.2",
        "@mapbox/mapbox-gl-geocoder": "^4.7.4",
        "@turf/clean-coords": "^6.5.0",
        "@turf/helpers": "^6.5.0",
        "@turf/truncate": "^6.5.0",
        "browserify": "^17.0.0",
        "budo": "^11.6.4",
        "d3": "^7.0.0",
        "d3-delaunay": "^6.0.2",
        "d3-scale": "^4.0.0",
